pipeline {
    agent any
    stages {
        stage("git-Checkout") {
            steps {
                echo "Performing Git Checkout"
            }
        }
        stage("Build") {
            steps {
                echo "Building"
            }
        }
        stage("Test") {
            steps {
                echo "Testing"
            }
        }
        stage("Deploy") {
            steps {
                echo "Deploying"
            }
        }
    }
}